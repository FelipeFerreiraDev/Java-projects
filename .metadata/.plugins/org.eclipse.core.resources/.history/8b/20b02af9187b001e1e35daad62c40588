package fundamentos;

/*
 * Hello world = nome da classe
 * public = tipo de acesso
 * class = tipo da estrtura/classe
 */

public class HelloWorld {
	// Todo conte√∫do deve ser escrito aqui dentro para manter o escopo
	public static void main(String[] args) {
		/*
		 * Numeros (int, double, float, long)
		 * Texto (String)
		 * Boleano (boolean)
		 */
		
		int typeInt = 10;
		double typeDouble = 3.14;
		float typeFloat = 3.14f;
		long typeLong = 1918191981891818l;
		String typeString = "Hello world";
		boolean typeBoolean = true;
	}
}
